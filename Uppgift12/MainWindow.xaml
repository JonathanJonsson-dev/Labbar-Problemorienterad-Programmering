        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift12"
        mc:Ignorable="d"
        Title="Medelvärde" Height="450" Width="800">
    <Grid>
        <ListBox x:Name ="listBoxValues"  HorizontalAlignment="Left" Height="160" Margin="480,109,0,0" VerticalAlignment="Top" Width="100" />
        <Label Content="Mata in tal" HorizontalAlignment="Left" Margin="275,83,0,0" VerticalAlignment="Top"/>
        <Button x:Name="okBtn" Content="Ok" HorizontalAlignment="Left" Margin="400,109,0,0" VerticalAlignment="Top" Width="75" Click="okBtn_Click"/>
        <TextBox x:Name="inmatningTextbox" HorizontalAlignment="Left" Height="23" Margin="275,109,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Totala medelvärdet" HorizontalAlignment="Left" Margin="275,137,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="medelvärdeTextbox" HorizontalAlignment="Left" Height="23" Margin="275,163,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
