        <Label Content="Sök efter" HorizontalAlignment="Left" Margin="315,211,0,0" VerticalAlignment="Top"/>
        <Label Content="Resultat" HorizontalAlignment="Left" Margin="469,242,0,0" VerticalAlignment="Top"/>
        <Label x:Name="resultatLabel" HorizontalAlignment="Left" Margin="469,273,0,0" VerticalAlignment="Top"/>
